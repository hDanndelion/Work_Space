    <div>
        <img id="sound" src="http://127.0.0.1:5500/Genshin_Impact/assets/img/kyhieumusic.PNG" alt="" onclick="change()">
    </div>
    <script>
        var a = document.getElementById("sound");
        function change() {
            if (a.src == "http://127.0.0.1:5500/Genshin_Impact/assets/img/kyhieumusic.PNG" ) {
                a.src = "http://127.0.0.1:5500/Genshin_Impact/assets/img/kyhieumusicoff.PNG";
            } else {
                a.src = "http://127.0.0.1:5500/Genshin_Impact/assets/img/kyhieumusic.PNG";
            }
        }
    </script>
